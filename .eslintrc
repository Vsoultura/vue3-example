{

  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
    // "airbnb-base"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "rules": {
    "semi": 2,
    "no-undef":"off",
    "no-unused-vars":"off",
    "vue/multi-word-component-names": "off"
    // 'no-console': 'off',
    // 'import/no-unresolved': 'off',
    // 'import/extensions': 'off',
    // 'import/no-extraneous-dependencies': 'off'
  }
}
